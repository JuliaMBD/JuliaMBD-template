{
    "name": "myjulia",
    "build": {
        "dockerfile": "../Dockerfile"
    },
    "customizations": {
        "vscode": {
            "extensions": [
				"mhutchie.git-graph",
				"julialang.language-julia",
				"hediet.vscode-drawio"
			],
            "settings": {
                "julia.executablePath": "/usr/local/julia/bin/julia",
                "editor.acceptSuggestionOnEnter": "off"
            }
        }
    }
}
